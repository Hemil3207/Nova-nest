  <script>
      const categoryItems = document.querySelectorAll('.category-item');
      const categoryTitle = document.getElementById('categoryTitle');
      const categoryDescription = document.getElementById('categoryDescription');

      const categoryInfo = {
          "Men's Fashion": {
              title: "üëî Men's Fashion",
              description: "Discover the latest trends in men's fashion including casual wear, formal suits, athletic wear, and accessories. From everyday essentials to premium collections, find the perfect style for every occasion and season."
          },
          "Women's Fashion": {
              title: "üëó Women's Fashion",
              description: "Explore our curated collection of women's fashion featuring traditional wear, contemporary styles, and global brands. Find dresses, sarees, tops, and accessories for every season, celebration, and lifestyle."
          },
          "Home & Kitchen": {
              title: "üè† Home & Kitchen",
              description: "Create your dream home with our comprehensive collection of kitchen appliances, home decor, bedding, and furniture. Quality products to make your house feel like home with style and comfort."
          },
          "Kids Fashion": {
              title: "üë∂ Kids Fashion",
              description: "Shop for comfortable and stylish clothing for children of all ages. From school uniforms to casual wear, sports gear to accessories, find everything your kids need to look great and feel comfortable."
          },
          "Beauty & Health": {
              title: "üíÑ Beauty & Health",
              description: "Enhance your natural beauty and wellness with our range of cosmetics, skincare, haircare, and health supplements. Trusted brands and premium products for daily self-care and personal wellness."
          },
          "Electronics": {
              title: "üì± Electronics",
              description: "Stay updated with the latest technology. Browse smartphones, laptops, tablets, gaming equipment, and accessories from leading brands. Get the best deals on tech products with warranty."
          },
          "Sports & Fitness": {
              title: "‚öΩ Sports & Fitness",
              description: "Achieve your fitness goals with our sports and fitness equipment. From gym equipment to outdoor gear, find everything you need to stay active, healthy, and fit."
          }
      };

      categoryItems.forEach(item => {
          item.addEventListener('mouseenter', function () {
              const categoryText = this.textContent.trim().split('\n')[1] || this.textContent.trim();
              const info = categoryInfo[categoryText];

              if (info) {
                  categoryTitle.textContent = info.title;
                  categoryDescription.textContent = info.description;
              }
          });
      });

      // Reset on mouse leave from category nav
      document.querySelector('.category-nav-content').addEventListener('mouseleave', function () {
          categoryTitle.textContent = 'Welcome to Nova nest';
          categoryDescription.textContent = 'Explore our wide range of products across all categories. Hover over any category to see our mega menu with extensive product selection and exclusive deals. We offer quality products at competitive prices with fast shipping and excellent customer service.';
      });
  </script>
